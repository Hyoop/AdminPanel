=\"overview\">\n        <section className=\"overview__header\">\n            <MainHeader>\n                <h1>Overview</h1>\n            </MainHeader>\n        </section>\n        {!isLoading && <section className=\"overviewcard\">\n            <Card title=\"Subscriptions\" content={NbSubscriptions}/>\n            <Card title=\"Members\" content={Nbmembers}/>\n            <Card title=\"Vegetables\" content={NbVegetables}/>\n            <Card title=\"Recipes\" content={NbRecipes}/>\n        </section>}\n        <section className=\"overview-weeks_vegetables\">\n            <WeeksVegetables showForm={showForm} myClick={handleAddVegetables}/>\n        </section>\n        <section className=\"overview-task\">\n        </section>\n        {showForm && \n            <CardAddForm text=\"ADD A VEGETABLE\" closePopup={handleAddVegetables}/>\n        }\n    </div>)\n    \n}\n\nexport default Overview;\n","/home/hyoshin/dev/panier-bio-adminpanel/src/pages/overview/components/WeeksVegetables/WeeksVegetables.tsx",["158","159"],"import { useContext, useEffect, useState } from \"react\";\nimport env from \"react-dotenv\";\nimport \"./WeeksVegetables.css\";\nimport CardVegetables from \"../Card_Vege/CardVegetables\";\nimport CardAdd from \"../Card_Vege/CardAdd\";\nimport { useHttpClient } from \"../../../../shared/hooks/http-hook\";\nimport { AuthContext } from \"../../../../shared/context/auth-context\";\n\nconst WeeksVegetables = (props: {\n  showForm: boolean,\n  myClick: () => void\n}) => \n{\n  const auth = useContext(AuthContext);\n  const [vegetables, setvegetables] = useState([]);\n\n  const { isLoading, sendRequest } = useHttpClient();\n  const [value, setValue] = useState<boolean>(props.showForm);\n\n  useEffect(() => {\n    setValue(props.showForm)\n    const fetchVegetables = async () => {\n      try {\n        const responseData = await sendRequest(\n          \"\" + env.apiURL + \"/api/panier/week/\",\n          \"GET\",\n          null,\n          {\n            \"Content-Type\" : \"application/json\",\n            Authorization: \"Bearer \" + auth.token\n          }\n        );\n        setvegetables(responseData.vegetables);\n      } catch (err) {}\n    };\n    fetchVegetables();\n  }, [sendRequest,props.showForm]);\n\n  return (\n    <div className=\"weeks\">\n      <div className=\"\">\n        <h1 className=\"weeks__header__title\">Week's vegetables</h1>\n        <h3 className=\"weeks__header__date\">as of 25 May 2019, 09:41 PM</h3>\n      </div>\n      <div className=\"weeks__vegetables\">\n        {vegetables.length <= 0 && !isLoading ? (\n          <p>No vegetables found.</p>\n        ) : null}\n        {!isLoading &&\n          vegetables.map((vegetable: {\n            _id: any,\n            name: string,\n            imageUrl: string\n          }) => (\n            <CardVegetables\n              key={vegetable._id}\n              name={vegetable.name}\n              image={vegetable.imageUrl}\n              \n            />\n          ))}\n         <CardAdd myClick={props.myClick} />\n      </div>\n    </div>\n  );\n};\n\nexport default WeeksVegetables;\n","/home/hyoshin/dev/panier-bio-adminpanel/src/pages/overview/components/Card_Vege/CardAddForm.tsx",[],"/home/hyoshin/dev/panier-bio-adminpanel/src/pages/overview/components/Card_Vege/CardVegetables.tsx",[],["160","161"],"/home/hyoshin/dev/panier-bio-adminpanel/src/pages/overview/components/Card_Vege/CardAdd.tsx",[],"/home/hyoshin/dev/panier-bio-adminpanel/src/pages/overview/components/Card/Card.tsx",[],"/home/hyoshin/dev/panier-bio-adminpanel/src/pages/vegetables/Vegetables.tsx",["162"],"import {useState, useEffect, useContext} from \"react\";\nimport \"./Vegetables.css\";\nimport Legume from \"./components/Legume\";\nimport Paginator from \"./components/Paginator\";\nimport Loader from \"../../shared/components/Loader/Loader\";\nimport MainHeader from \"../../shared/components/Navigation/MainHeader\";\n\nimport { useHttpClient } from \"../../shared/hooks/http-hook